{
  "name": "proon",
  "version": "0.0.10",
  "description": "A neatly trimmed data-tree API for many species of storage",
  "main": "build/proon.js",
  "directories": {
    "doc": "doc",
    "test": "test",
    "example": "usage"
  },
  "devDependencies": {
    "apage": "latest",
    "coffee-script": "latest",
    "nodemon": "latest"
  },
  "dependencies": {
  },
  "keywords": [
    "CoffeeScript",
    "data-tree",
    "localStorage",
    "sessionStorage",
    "storage",
    "filesystem",
    "fs",
    "database",
    "db",
    "Document Object Model",
    "DOM",
    "object"
  ],
  "scripts": {
    "hi": "open .; open index.html; open test/run-test.html; subl .",
    "make": "node bin/akaybe-build",
    "index": "node_modules/apage/bin/apage -i README.md doc/*.md -o index.html -p \"`cat node_modules/apage/build/plugin/*.js`\"",
    "test": "node test/run-test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/richplastow/proon.git"
  },
  "author": "Rich Plastow <rich@richplastow.com> (http://richplastow.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/richplastow/proon/issues"
  },
  "homepage": "http://proon.richplastow.com/"
}
